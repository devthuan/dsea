{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@components": "./src/components",
          "@layouts": "./src/layouts",
          "@services": "./src/services",
          "@config": "./src/config",
          "@hooks": "./src/hooks",
          "@pages": "./src/pages",
          "@redux": "./src/redux",
          "@routes": "./src/routes",
          "@utils": "./src/utils",
          "@assets": "./src/assets",
          "~": "./src"
        }
      }
    ]
  ]
}
